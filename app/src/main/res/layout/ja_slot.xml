<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@drawable/main_paid"
    android:orientation="vertical">

    <TableLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:background="@drawable/modity_splish"
        android:paddingStart="13dp"
        android:paddingTop="10dp"
        android:paddingEnd="13dp"
        android:paddingBottom="10dp">

        <TableRow
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:orientation="horizontal">

            <com.gladness.merriment.treat.hilaritypro.OVDPrizesView
                android:id="@+id/symbol00"
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                android:layout_margin="3px"
                android:layout_weight="2"
                android:scaleType="fitCenter"
                android:src="@drawable/activity_distribution_splish" />

            <com.gladness.merriment.treat.hilaritypro.OVDPrizesView
                android:id="@+id/symbol10"
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                android:layout_margin="3px"
                android:layout_weight="2"
                android:src="@drawable/modity_login_android" />

            <com.gladness.merriment.treat.hilaritypro.OVDPrizesView
                android:id="@+id/symbol20"
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                android:layout_margin="3px"
                android:layout_weight="2"
                android:src="@drawable/entry_launcher_build" />


            <com.gladness.merriment.treat.hilaritypro.OVDPrizesView
                android:id="@+id/symbol30"
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                android:layout_margin="3px"
                android:layout_weight="2"
                android:src="@drawable/config_prizes" />

            <com.gladness.merriment.treat.hilaritypro.OVDPrizesView
                android:id="@+id/symbol40"
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                android:layout_margin="3px"
                android:layout_weight="2"
                android:src="@drawable/selector_blue" />
        </TableRow>

        <TableRow
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:orientation="horizontal">


            <com.gladness.merriment.treat.hilaritypro.OVDPrizesView
                android:id="@+id/symbol01"
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                android:layout_margin="3px"
                android:layout_weight="2"
                android:src="@drawable/prizes_payout" />

            <com.gladness.merriment.treat.hilaritypro.OVDPrizesView
                android:id="@+id/symbol11"
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                android:layout_margin="3px"
                android:layout_weight="2"
                android:src="@drawable/droplet_percent" />

            <com.gladness.merriment.treat.hilaritypro.OVDPrizesView
                android:id="@+id/symbol21"
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                android:layout_margin="3px"
                android:layout_weight="2"
                android:src="@drawable/styles_splish" />

            <com.gladness.merriment.treat.hilaritypro.OVDPrizesView
                android:id="@+id/symbol31"
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                android:layout_margin="3px"
                android:layout_weight="2"
                android:src="@drawable/distribution_login" />

            <com.gladness.merriment.treat.hilaritypro.OVDPrizesView
                android:id="@+id/symbol41"
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                android:layout_margin="3px"
                android:layout_weight="2"
                android:src="@drawable/distribution_screen_entry" />

        </TableRow>

        <TableRow
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:orientation="horizontal">

            <com.gladness.merriment.treat.hilaritypro.OVDPrizesView
                android:id="@+id/symbol02"
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                android:layout_margin="3px"
                android:layout_weight="2"
                android:src="@drawable/activity_distribution_splish" />

            <com.gladness.merriment.treat.hilaritypro.OVDPrizesView
                android:id="@+id/symbol12"
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                android:layout_margin="3px"
                android:layout_weight="2"
                android:src="@drawable/modity_login_android" />

            <com.gladness.merriment.treat.hilaritypro.OVDPrizesView
                android:id="@+id/symbol22"
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                android:layout_margin="3px"
                android:layout_weight="2"
                android:src="@drawable/entry_launcher_build" />

            <com.gladness.merriment.treat.hilaritypro.OVDPrizesView
                android:id="@+id/symbol32"
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                android:layout_margin="3px"
                android:layout_weight="2"
                android:src="@drawable/config_prizes" />

            <com.gladness.merriment.treat.hilaritypro.OVDPrizesView
                android:id="@+id/symbol42"
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                android:layout_margin="3px"
                android:layout_weight="2"
                android:src="@drawable/selector_blue" />
        </TableRow>
    </TableLayout>


    <TableLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:padding="10dp">

        <TableRow
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:orientation="horizontal">

            <TextView
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                android:layout_weight="2"
                android:gravity="center_horizontal"
                android:text="BET:"
                android:textColor="@color/combinationGradleLogin" />

            <TextView
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                android:layout_weight="2"
                android:gravity="center_horizontal"
                android:text="LINES:"
                android:textColor="@color/combinationGradleLogin" />

            <TextView
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                android:layout_weight="2"
                android:gravity="center_horizontal"
                android:text="TOTAL\n   BET:"
                android:textColor="@color/combinationGradleLogin" />

            <TextView
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                android:layout_weight="2"
                android:gravity="center_horizontal"
                android:text="WINNER\n    PAID:"
                android:textColor="@color/combinationGradleLogin" />

            <TextView
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                android:layout_weight="2"
                android:gravity="center_horizontal"
                android:text="CREDIT:"
                android:textColor="@color/combinationGradleLogin" />

        </TableRow>

        <TableRow
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:orientation="horizontal">

            <TextView
                android:id="@+id/single_bet_value"
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                android:layout_weight="2"
                android:gravity="center_horizontal"
                android:text="00"
                android:textColor="@color/selectorPinc" />

            <TextView
                android:id="@+id/active_lines_value"
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                android:layout_weight="2"
                android:gravity="center_horizontal"
                android:text="00"
                android:textColor="@color/selectorPinc" />

            <TextView
                android:id="@+id/total_bet_value"
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                android:layout_weight="2"
                android:gravity="center_horizontal"
                android:text="0000"
                android:textColor="@color/selectorPinc" />

            <TextView
                android:id="@+id/winner_paied_value"
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                android:layout_weight="2"
                android:gravity="center_horizontal"
                android:text="000000"
                android:textColor="@color/selectorPinc" />

            <TextView
                android:id="@+id/credit_value"
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                android:layout_weight="2"
                android:gravity="center_horizontal"
                android:text="000000"
                android:textColor="@color/selectorPinc" />

        </TableRow>
    </TableLayout>

    <TableLayout
        android:layout_marginTop="10dp"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:padding="3dp">

        <TableRow
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:orientation="horizontal">

            <Button
                android:enabled="false"
                android:id="@+id/autoplay_button"
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                android:layout_weight="2"
                android:text="@string/settingsDropletManifest"
                android:textColor="@color/symbolBase" />

            <Button
                android:enabled="false"
                android:id="@+id/payout_button"
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                android:layout_weight="2"
                android:text="@string/dropletPayout"
                android:textColor="@color/symbolBase" />

            <Button

                android:id="@+id/lines_button"
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                android:layout_weight="2"
                android:text="@string/register_9qNews"
                android:textColor="@color/symbolBase" />


        </TableRow>

        <TableRow
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:orientation="horizontal">

            <Button
                android:id="@+id/max_bet_button"
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                android:layout_weight="2"
                android:text="@string/baseAndroidConfig"
                android:textColor="@color/symbolBase" />

            <Button
                android:id="@+id/bet_button"
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                android:layout_weight="2"
                android:text="@string/settingsPrizeEntry"
                android:textColor="@color/symbolBase" />


            <Button
                android:id="@+id/spin_button"
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                android:layout_weight="2"
                android:text="@string/reelsReels"
                android:textColor="@color/symbolBase" />
        </TableRow>
    </TableLayout>

</LinearLayout>